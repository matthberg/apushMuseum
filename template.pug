doctype html
html
 head
  title= title
  for styleURL in stylesURLs
   link(rel='stylesheet',src=styleURL,type='text/css')
 body
  header
   img(src=logoURL,alt=title)
   nav
    ul
     for name in links
      li: a(href='#'+name) #{name}
  block content
  footer
   ul
    li= title
    for name in links
     li: a(href='#'+name) #{name}
    li made by #{author}
    li &copy;#{year}
  for scriptURL in scriptsURLs
   script(src=scriptURL,type="text/javascript")