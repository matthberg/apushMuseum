include ./mixins.pug
doctype html
html
 head
  title= title
  for styleURL in stylesURLs
   link(rel='stylesheet',href="./css/"+styleURL,type='text/css')
 body
  header
   block header
   ul.inlineNav
    for name in links
     li: a(href='#'+name)= name 
  block content
  footer
   ul.inlineNav
    li: a(href='#')= title
    for name in links
     li: a(href='#'+name)= name 
    li made by #{author}
    li &copy;#{year}
  script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.2/jquery.min.js",type="text/javascript")
  for scriptURL in scriptsURLs
   script(src="./js/"+scriptURL,type="text/javascript")
